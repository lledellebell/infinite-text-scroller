{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "es2020": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended"
  ],
  "rules": {
    "no-var": "off",
    "prefer-const": "off",
    "prefer-arrow-callback": "off",
    "object-shorthand": "off",
    "no-unused-vars": ["warn"],
    "no-console": "off"
  }
}